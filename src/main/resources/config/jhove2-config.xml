<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:util="http://www.springframework.org/schema/util"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-2.5.xsd
		   http://www.springframework.org/schema/util 
		   http://www.springframework.org/schema/util/spring-util-2.0.xsd">

	<!-- JHOVE2 framework bean -->
	<bean id="JHOVE2" class="org.jhove2.core.JHOVE2" scope="prototype">
		<property name="characterizerModule" ref="CharacterizerModule"/>
		<property name="dispatcherModule" ref="DispatcherModule"/>
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	</bean>
		
		
		
 	<!-- Characterizer module bean -->
	<bean id="CharacterizerModule" 
	      class="org.jhove2.module.characterize.CharacterizerModule"
	      scope="prototype">
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	</bean>
	      
	      
	      
	<!-- Identifier module bean -->
	<bean id="IdentifierModule"
	      class="org.jhove2.module.identify.IdentifierModule"
	      scope="prototype">
	    <property name="wrappedProduct" ref="DROIDProduct"/>
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	</bean>
	      
	<!-- DROID product information bean -->
	<bean id="DROIDProduct" class="org.jhove2.module.identify.DROID"
	      scope="singleton">
		<constructor-arg type="java.lang.String" value="4.0"/>
		<constructor-arg type="java.lang.String" value="2009-06-09"/>
		<constructor-arg type="java.lang.String" value="Copyright 2008 by the National Archives. Available under the terms of the BSD license."/>
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	</bean>
	   
	   
	      
	<!-- Dispatcher module bean -->
	<bean id="DispatcherModule"
	      class="org.jhove2.module.dispatch.DispatcherModule"
	      scope="prototype">
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	</bean>
	      	      
	      
	      
	<!-- Aggrefier module bean -->
	<bean id="AggrefierModule"
	      class="org.jhove2.module.identify.AggrefierModule"
	      scope="prototype">
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	</bean>
	      
	      
	      
	<!--  Message digester module bean -->
	<bean id="DigesterModule"
	      class="org.jhove2.module.digest.DigesterModule"
	      scope="prototype">
		<property name="arrayDigesters">
	    	<list value-type="org.jhove2.module.digest.ArrayDigester">
	    		<ref bean="CRC32Digester"/>
	    	</list>
	    </property>
		<property name="bufferDigesters">
			<list value-type="org.jhove2.module.digest.BufferDigester">
				<ref bean="MD5Digester"/>
				<ref bean="SHA1Digester"/>
			</list>
		</property>
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	</bean>

	<!-- Adler-32 message digester bean -->
	<bean id="Adler32Digester" class="org.jhove2.module.digest.Adler32Digester"
	      scope="prototype"/>
	      			
	<!-- CRC-32 message digester bean -->
	<bean id="CRC32Digester" class="org.jhove2.module.digest.CRC32Digester"
	      scope="prototype"/>
		
	<!-- MD2 message digester bean -->
	<bean id="MD2Digester" class="org.jhove2.module.digest.MD2Digester"
	      scope="prototype"/>
	      	
	<!-- MD5 message digester bean -->
	<bean id="MD5Digester" class="org.jhove2.module.digest.MD5Digester"
	      scope="prototype"/>
	      
	<!-- SHA-1 message digester bean -->
	<bean id="SHA1Digester" class="org.jhove2.module.digest.SHA1Digester"
	      scope="prototype"/>
	      	      
	<!-- SHA=256 message digester bean -->
	<bean id="SHA256Digester" class="org.jhove2.module.digest.SHA256Digester"
	      scope="prototype"/>
	      	      
	<!-- SHA-384 message digester bean -->
	<bean id="SHA384Digester" class="org.jhove2.module.digest.SHA384Digester"
	      scope="prototype"/>
	      	      
	<!-- SHA-512 message digester bean -->
	<bean id="SHA512Digester" class="org.jhove2.module.digest.SHA512Digester"
	      scope="prototype"/>
	      
	      
	      
 	<!-- JSON displayer bean -->
	<bean id="JSONDisplayer" class="org.jhove2.module.display.JSONDisplayer"
	      scope="prototype">
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	</bean>
	      	
 	<!-- Text displayer bean -->
	<bean id="TextDisplayer" class="org.jhove2.module.display.TextDisplayer"
	      scope="prototype">
	    <property name="showIdentifiers" value="false"/>
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	</bean>
	      	
 	<!-- XML displayer bean -->
	<bean id="XMLDisplayer" class="org.jhove2.module.display.XMLDisplayer"
	      scope="prototype">
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	</bean>
	
	
	  
  	<!-- ASCII profile bean -->
	<bean id="ASCIIProfile" class="org.jhove2.module.format.utf8.ascii.ASCIIProfile"
	      scope="prototype">
	    <constructor-arg ref="ASCIIFormat"/>
	    <property name="developer" ref="CDLAgent"/>
	</bean>
	
	<!-- ASCII format bean -->
	<bean id="ASCIIFormat" class="org.jhove2.core.Format" scope="singleton">
		<constructor-arg type="java.lang.String" value="ASCII"/>
		<constructor-arg ref="ASCIIIdentifier"/>
		<constructor-arg type="org.jhove2.core.Format$Type" value="Format"/>
		<constructor-arg type="org.jhove2.core.Format$Ambiguity" value="Unambiguous"/>
		<property name="aliasIdentifiers">
			<set value-type="org.jhove2.core.I8R">
				<ref bean="ASCIIANSIX34"/>
				<ref bean="ASCIIECMA6"/>
				<ref bean="ASCIIISO646"/>
				<ref bean="ASCIIITUT50"/>
				<ref bean="ASCIIMIMEType"/>
				<ref bean="ASCIIRFC1345"/>
			</set>
		</property>
	</bean>
		
	<!-- ASCII identifier bean -->  
	<bean id="ASCIIIdentifier" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="info:jhove2/format/ascii"/>
	</bean>
	<!-- ASCII ANSI X3.4-1968 bean  -->
	<bean id="ASCIIANSIX34" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="ANSI X3.4-1968"/>
		<constructor-arg type="org.jhove2.core.I8R$Namespace" value="ANSI"/>
	</bean>
	<!-- ASCII ECMA-6 -->
	<bean id="ASCIIECMA6" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="ECMA-6"/>
		<constructor-arg type="org.jhove2.core.I8R$Namespace" value="ECMA"/>
	</bean>
	<!-- ASCII ISO 646 -->
	<bean id="ASCIIISO646" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="ISO/IEC 646:1991"/>
		<constructor-arg type="org.jhove2.core.I8R$Namespace" value="ISO"/>
	</bean>
	<!-- ASCII ITU-T T.50 -->
	<bean id="ASCIIITUT50" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="ITU-T T.50"/>
		<constructor-arg type="org.jhove2.core.I8R$Namespace" value="ITU"/>
	</bean>
	<!-- ASCII MIME type bean -->
	<bean id="ASCIIMIMEType" class="org.jhove2.core.I8R" scope="singleton">
	    <constructor-arg type="java.lang.String" value="text/plain; charset=&quot;US-ASCII&quot;"/>
	    <constructor-arg type="org.jhove2.core.I8R$Namespace" value="MIME"/>
	</bean>	
	<!--  ASCII RFC 1345 bean -->
	<bean id="ASCIIRFC1345" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="RFC 1345"/>
		<constructor-arg type="org.jhove2.core.I8R$Namespace" value="RFC"/>
	</bean>
	
	
	
	<!-- Bytestream format bean -->
	<bean id="BytestreamFormat" class="org.jhove2.core.Format" scope="singleton">
		<constructor-arg type="java.lang.String" value="Bytestream"/>
		<constructor-arg ref="BytestreamIdentifier"/>
		<constructor-arg type="org.jhove2.core.Format$Type" value="Format"/>
		<constructor-arg type="org.jhove2.core.Format$Ambiguity" value="Unambiguous"/>
		<property name="aliasIdentifiers" ref="BytestreamMIMEType"/>
		<property name="note" value="A Bytestream is a JHOVE2-defined format representing arbitrary data."/>
	</bean>
		
	<!-- Bytestream identifier bean -->  
	<bean id="BytestreamIdentifier" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="info:jhove2/format/bytestream"/>
	</bean>
	<!-- Bytestream MIME type bean -->
	<bean id="BytestreamMIMEType" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="application/octet-stream"/>
		<constructor-arg type="org.jhove2.core.I8R$Namespace" value="MIME"/>
	</bean>
	
	
	
	<!-- Clump module bean -->
	<bean id="ClumpModule" class="org.jhove2.module.format.clump.ClumpModule"
	      scope="prototype">
	    <constructor-arg ref="ClumpFormat"/>
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	    <property name="note" value="A Clump is JHOVE2-defined format that is a set of files presumed to collectively form a single logical object."/>
	</bean>
			  
	<!-- Clump format bean -->
	<bean id="ClumpFormat" class="org.jhove2.core.Format" scope="singleton">
		<constructor-arg type="java.lang.String" value="Clump"/>
		<constructor-arg ref="ClumpIdentifier"/>
		<constructor-arg type="org.jhove2.core.Format$Type" value="Format"/>
		<constructor-arg type="org.jhove2.core.Format$Ambiguity" value="Unambiguous"/>
	</bean>
	
   <!-- Clump identifier bean -->  
   <bean id="ClumpIdentifier" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="info:jhove2/format/clump"/>
   </bean>
     	
     	
     	
	<!-- Directory module bean -->
	<bean id="DirectoryModule" class="org.jhove2.module.format.directory.DirectoryModule"
	      scope="prototype">
	    <constructor-arg ref="DirectoryFormat"/>
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	    <property name="note" value="A Directory is a JHOVE2-defined format that is a file system directory."/>
	</bean>
	
	<!-- Directory format bean -->
	<bean id="DirectoryFormat" class="org.jhove2.core.Format" scope="singleton">
		<constructor-arg type="java.lang.String" value="Directory"/>
		<constructor-arg ref="DirectoryIdentifier"/>
		<constructor-arg type="org.jhove2.core.Format$Type" value="Format"/>
		<constructor-arg type="org.jhove2.core.Format$Ambiguity" value="Unambiguous"/>
	</bean>
				
	<!-- Directory identifier bean -->  
	<bean id="DirectoryIdentifier" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="info:jhove2/format/directory"/>
	</bean>
	
	
	   	
	<!-- File set module bean -->
	<bean id="FileSetModule" class="org.jhove2.module.format.fileset.FileSetModule"
	      scope="prototype">
	    <constructor-arg ref="FileSetFormat"/>
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	    <property name="note" value="A File Set is a JHOVE2-defined format that is a set of otherwise unrelated files, for example, the set of files specified on the JHOVE2 command line."/>
	</bean>
	
	<!-- File set format bean -->
	<bean id="FileSetFormat" class="org.jhove2.core.Format" scope="singleton">
		<constructor-arg type="java.lang.String" value="FileSet"/>
		<constructor-arg ref="FileSetIdentifier"/>
		<constructor-arg type="org.jhove2.core.Format$Type" value="Format"/>
		<constructor-arg type="org.jhove2.core.Format$Ambiguity" value="Unambiguous"/>
	</bean>
				
	<!-- File set identifier bean -->  
	<bean id="FileSetIdentifier" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="info:jhove2/format/file-set"/>
	</bean>

		
	
	<!-- Shapefile module bean -->
	<bean id="ShapefileModule" class="org.jhove2.module.format.shapefile.ShapefileModule"
	      scope="prototype">
	    <constructor-arg ref="ShapefileFormat"/>
	    <property name="developer" ref="StanfordAgent"/>
	</bean>
      
    <!-- Shapefile format bean -->
	<bean id="ShapefileFormat" class="org.jhove2.core.Format" scope="singleton">
		<constructor-arg type="java.lang.String" value="Shapefile"/>
		<constructor-arg ref="ShapefileIdentifier"/>
		<constructor-arg type="org.jhove2.core.Format$Type" value="Format"/>
		<constructor-arg type="org.jhove2.core.Format$Ambiguity" value="Unambiguous"/>
	</bean>
		
	<!-- Shapefile identifier bean -->  
	<bean id="ShapefileIdentifier" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="info:jhove2/format/shapefile"/>
	</bean>
			
	
	
	<!-- UTF-8 module bean -->
	<bean id="UTF8Module" class="org.jhove2.module.format.utf8.UTF8Module"
	      scope="prototype">
	    <constructor-arg ref="UTF8Format"/>
	    <property name="profile" ref="ASCIIProfile"/>
	    <property name="developer" ref="CDLAgent"/>
	</bean>

	<bean id="UTF8Character" class="org.jhove2.module.format.utf8.UTFCharacter"
	      scope="prototype"/>
   
	<!-- UTF-8 format bean -->
	<bean id="UTF8Format" class="org.jhove2.core.Format" scope="singleton">
		<constructor-arg type="java.lang.String" value="UTF-8"/>
		<constructor-arg ref="UTF8Identifier"/>
		<constructor-arg type="org.jhove2.core.Format$Type" value="Family"/>
		<constructor-arg type="org.jhove2.core.Format$Ambiguity" value="Unambiguous"/>
		<property name="aliasIdentifiers">
			<set value-type="org.jhove2.core.I8R">
				<ref bean="UTF8MIMEType"/>
				<ref bean="UTF8RFC3629"/>
			</set>
		</property>
		<property name="aliasNames">
			<set>
				<value>UCS Transformation Format, 8 bit</value>
				<value>Unicode Transformation Format, 8 bit</value>
			</set>
		</property>
	</bean>
		
	<!-- UTF-8 identifier bean -->  
	<bean id="UTF8Identifier" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="info:jhove2/format/utf-8"/>
	</bean>
	<!-- UTF-8 MIME type bean -->
	<bean id="UTF8MIMEType" class="org.jhove2.core.I8R" scope="singleton">
	    <constructor-arg type="java.lang.String" value="text/plain; charset=&quot;UTF-8&quot;"/>
	    <constructor-arg type="org.jhove2.core.I8R$Namespace" value="MIME"/>
	</bean>
	<!--  UTF-8 RFC 3629 bean -->
	<bean id="UTF8RFC3629" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="RFC 3629"/>
		<constructor-arg type="org.jhove2.core.I8R$Namespace" value="RFC"/>
	</bean>
		     	
     	
  	
	<!-- Zip module bean -->
	<bean id="ZipModule" class="org.jhove2.module.format.zip.ZipModule"
	      scope="prototype">
	    <constructor-arg ref="ZipFormat"/>
		<property name="developers">
			<list value-type="org.jhove2.core.Agent">
				<ref bean="CDLAgent"/>
				<ref bean="PorticoAgent"/>
				<ref bean="StanfordAgent"/>
			</list>
		</property>
	</bean>
      
	<!-- Zip format bean -->
	<bean id="ZipFormat" class="org.jhove2.core.Format" scope="singleton">
		<constructor-arg type="java.lang.String" value="Zip"/>
		<constructor-arg ref="ZipIdentifier"/>
		<constructor-arg type="org.jhove2.core.Format$Type" value="Family"/>
		<constructor-arg type="org.jhove2.core.Format$Ambiguity" value="Unambiguous"/>
	</bean>
		
	<!-- Zip identifier bean -->  
	<bean id="ZipIdentifier" class="org.jhove2.core.I8R" scope="singleton">
		<constructor-arg type="java.lang.String" value="info:jhove2/format/zip"/>
	</bean>
	
	
	
	<!-- CDL agent bean -->
	<bean id="CDLAgent" class="org.jhove2.core.Agent" scope="singleton">
		<constructor-arg type="java.lang.String" value="California Digital Library"/>
		<constructor-arg type="org.jhove2.core.Agent$Type" value="Corporate"/>
		<property name="URI" value="http://www.cdlib.org/"/>
	</bean>
		
	<!-- Portico agent bean -->
	<bean id="PorticoAgent" class="org.jhove2.core.Agent" scope="singleton">
		<constructor-arg type="java.lang.String" value="Portico"/>
		<constructor-arg type="org.jhove2.core.Agent$Type" value="Corporate"/>
		<property name="URI" value="http://www.portico.org/"/>
	</bean>
		
	<!-- Stanford agent bean -->
	<bean id="StanfordAgent" class="org.jhove2.core.Agent" scope="singleton">
		<constructor-arg type="java.lang.String" value="Stanford University"/>
		<constructor-arg type="org.jhove2.core.Agent$Type" value="Corporate"/>
		<property name="URI" value="http://www.stanford.edu/"/>
	</bean>
	
	
	
	<!-- C0 Control properties bean -->
	<util:properties id="C0Control"	
		             location="classpath:properties/unicode/c0control.properties"/>
	
	<!-- C1 Control properties bean -->
	<util:properties id="C1Control"	
		             location="classpath:properties/unicode/c1control.properties"/>

	<!-- Codeblock properties bean -->
	<util:properties id="CodeBlock"	
		             location="classpath:properties/unicode/codeblock.properties"/>
		
	<!-- Displayer properties bean -->
	<util:properties id="Displayer"
	                 location="classpath:properties/displayer.properties"/>

	<!-- Dispatcher properties bean -->
	<util:properties id="Dispatcher"	
		             location="classpath:properties/dispatcher.properties"/>
		
</beans>
